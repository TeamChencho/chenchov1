<!--Abrir o cerra loader-->
<script type="text/javascript">
  function loader(abrir){
    if(abrir){
      $("#spinner").fadeIn(1);
      $("#spinner").modal('show')
      $('.modal-backdrop').show()
    }else{
      $("#spinner").fadeOut(1);
      $('.modal-backdrop').hide()
      $('.modal-open').css({'overflow': 'visible'});
    }
  }
</script>

