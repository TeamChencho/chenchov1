      if( addText ){
        $("#list_phases_production_line_tamplate_to_save #item span").each(function ( index ) {
            $(this).text("Step "+ number)
            number++
        })
      }else{
        $("#list_phases_production_line_tamplate #item span").each(function ( index ) {
            $(this).text("")
        })
      }
    }

</script>

