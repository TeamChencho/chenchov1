<script type="text/javascript">
  function saveData (data, url, pills) {
    
    Swal.fire({
      title: 'To update!',
      text: 'Do you want to update this data',
      icon: 'warning',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      cancelButtonText: 'Cancel',
      confirmButtonText: 'To Update'
    }).then((result) => {
      if (result.value) {
        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function(response) {
              $("#spinner").fadeOut(1);
              $('.modal-backdrop').hide()
              $('.modal-open').css({'overflow': 'visible'});
              Swal.fire(
                'Saved!',
                'The data has been saved successfully',
                'success'
              )
              toggleTables(pills)
            },error : function(XMLHttpRequest, textStatus, errorThrown) {
              $("#spinner").fadeOut(1);
              $('.modal-backdrop').hide()
              $('.modal-open').css({'overflow': 'visible'});
              log(textStatus.responseJSON.error)
              if (textStatus == "") {
                alert("Error with the database connection. Please check your internet connection")
              }
            }
        })
      }else{
        $("#spinner").fadeOut(1);
        $('.modal-backdrop').hide()
        $('.modal-open').css({'overflow': 'visible'});
      }
    })

  }
</script>

