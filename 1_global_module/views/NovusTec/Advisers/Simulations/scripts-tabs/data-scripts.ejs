  $('#form_agregar_familia').submit(function(event) {
    // submit the form
    event.preventDefault();
    Swal.fire({
      title: 'Add!',
      text: 'Do you want to add this product family?',
      icon: 'warning',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      cancelButtonText: 'Cancel',
      confirmButtonText: 'Add'
    }).then((result) => {
      if (result.value) {
        $(this).ajaxSubmit({
          type: 'POST',
          url: '/advisers/simulations/master_data/product_family/datos_agregar/<%= simulation.id %>',
          success: function(response, textStatus, jqXHR) {
            // The form was successfully submitted  
            log(response)
            var data = response.data
            var dataUniq = response.dataUniq
            log(dataUniq.length )
            if( dataUniq.length <= 0 ){     
              ajaxTablePF(true)
              Swal.fire(
                'Added!',
                'The product family has been added successfully',
                'success'
              )
              $("#form_agregar_familia").trigger("reset");
            }else{

              
              var textRMT = ''

              for(var i = 0; i < dataUniq.length; i++){
                if(i==dataUniq.length-1){
                  textRMT +=  dataUniq[i]["nombre"]
                }else{
                  textRMT +=  dataUniq[i]["nombre"]+', '
                }
              }

